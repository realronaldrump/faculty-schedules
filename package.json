{
  "name": "hsd-faculty-dashboard",
  "private": true,
  "version": "2.0.0",
  "type": "module",
  "description": "Baylor University Human Sciences & Design Faculty Dashboard with Normalized Database Structure",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "lint": "eslint . --ext js,jsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "migrate": "node scripts/migrate.js",
    "migrate-programs": "node migration-script.js",
    "clear-programs": "node clear-faculty-programs.js",
    "validate-data": "node scripts/validate-data.js",
    "backup-data": "node scripts/backup-data.js",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "emulators": "firebase emulators:start",
    "deploy": "firebase deploy",
    "deploy:hosting": "firebase deploy --only hosting",
    "deploy:firestore": "firebase deploy --only firestore"
  },
  "dependencies": {
    "clsx": "^2.1.1",
    "date-fns": "^3.6.0",
    "firebase": "^10.14.1",
    "html2canvas": "^1.4.1",
    "jspdf": "^3.0.1",
    "jspdf-autotable": "^5.0.2",
    "lucide-react": "^0.513.0",
    "papaparse": "^5.5.3",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-drag-drop-files": "^3.1.0",
    "react-hook-form": "^7.57.0",
    "react-hot-toast": "^2.5.2",
    "react-query": "^3.39.3",
    "react-router-dom": "^6.30.1",
    "uuid": "^9.0.1",
    "xlsx": "^0.18.5",
    "zustand": "^4.5.7"
  },
  "devDependencies": {
    "@tailwindcss/container-queries": "^0.1.1",
    "@tailwindcss/forms": "^0.5.10",
    "@tailwindcss/typography": "^0.5.16",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^14.3.1",
    "@types/react": "^18.3.23",
    "@types/react-dom": "^18.3.7",
    "@vitejs/plugin-react": "^4.5.1",
    "autoprefixer": "^10.4.21",
    "eslint": "^8.57.1",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-react-refresh": "^0.4.20",
    "firebase-tools": "^13.35.1",
    "jsdom": "^23.2.0",
    "postcss": "^8.5.4",
    "prettier": "^3.5.3",
    "prettier-plugin-tailwindcss": "^0.6.12",
    "tailwindcss": "^3.4.17",
    "vite": "^5.4.19",
    "vitest": "^1.6.1"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "keywords": [
    "react",
    "firebase",
    "firestore",
    "tailwindcss",
    "faculty-management",
    "scheduling",
    "university",
    "baylor",
    "dashboard"
  ],
  "author": {
    "name": "Davis Deaton",
    "email": "davis_deaton@baylor.edu",
    "affiliation": "Baylor University - Human Sciences & Design"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/your-org/hsd-faculty-dashboard.git"
  },
  "bugs": {
    "url": "https://github.com/your-org/hsd-faculty-dashboard/issues"
  },
  "homepage": "https://your-org.github.io/hsd-faculty-dashboard",
  "license": "MIT"
}
